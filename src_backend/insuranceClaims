const mongoose = require('mongoose')
const InsuranceClaim = new mongoose.Schema({
    "claimID": Number,
    "insuranceID": Number,
    "firstName": String,
    "lastName": String,
    "expenseDate": String,
    "amount": Number,
    "purpose": String,
    "followUp": Boolean,
    "previousClaimID": Number,
    "status": String,
    "lastEditedClaimDate": String
})

module.exports(InsuranceClaim)